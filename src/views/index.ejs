<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Document</title>
    </head>
    <body>
        <table>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Email</th>
                <th>City</th>
                <th>Delete</th>
                <th>Edit</th>
            </tr>
            <% data.forEach((item,index) => { %>
            <tr>
                <td><%= item.id %></td>
                <td><%= item.name %></td>
                <td><%= item.email %></td>
                <td><%= item.city %></td>
                <td><a class="delete" href="/delete-user/<%= item.id%>" class="delete" id="<%= item.id%>">X</a></td>
                <td><a class="btn-update" href="/update-user/<%= item.id%>">update</a></td>
            </tr>
            <% }); %>
        </table>
        <h1>Hello Ejs</h1>
        <img src="/img/gai.jpg" alt="" style="width: 100px; height: 100px" />
    </body>
</html>
