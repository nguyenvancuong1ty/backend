{
    "version": 2,
    "builds": [
        {
            "src": "src/build/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["src/build/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/build/index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "https://nguyenvancuong1ty.github.io/FE_clone_Egacake/"
            }
        }
    ]
}
